1#include <stdio.h>
 2
 3/**
 4 *main - prints the first 98 fibonacci numbers starting with 1 and 2.
 5 *Return: 0
 6 */
 7
 8int main(void)
 9{
10        unsigned long int n1 = 1, n2 = 1, s = 0;
11        long int n1_h, n2_h, n1_t, n2_t, s_h, s_t;
12        int control, splitter;
13
14        printf("1");
15
16        for (control = 2; control < 93; control++)
17        {
18                s = (n1 + n2);
19                n1 = n2;
20                n2 = s;
21
22                printf(", %lu", s);
23        }
24
25        n1_h = (n1 / 1000000000);
26        n1_t = (n1 % 1000000000);
27        n2_h = (n2 / 1000000000);
28        n2_t = (n2 % 1000000000);
29
30        for (; control < 99; control++)
31        {
32                splitter = (n1_t + n2_t) / 1000000000;
33                s_t = (n1_t + n2_t) / (1000000000 * splitter);
34                s_h = (n1_h + n2_h) + splitter;
35
36                printf(", %lu%lu", s_h, s_t);
37
38                n1_h = n2_h;
39                n1_t = n2_t;
40                n2_h = s_h;
41                n2_t = s_t;
42        }
43
44        printf("\n");
45
46        return (0);
47}1#include <stdio.h>
